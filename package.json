{
  "name": "vue-hui",
  "description": "A mobile components Library with Vue2.js from HoldSkill.",
  "version": "0.0.3",
  "author": "lee",
  "main": "dist/hui.js",
  "style": "dist/hui.css",
  "keywords": [
    "HUI App",
    "ui",
    "hui",
    "vue",
    "vue.js",
    "vue2.js",
    "component",
    "components",
    "framework",
    "vue2 ui",
    "vue2 components",
    "mobile",
    "mobile ui"
  ],
  "files": [
    "dist"
  ],
  "homepage": "http://vue.hui.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/holdskill/vue-hui.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev": "webpack-dev-server --content-base example/ --port 4321 --inline --hot --host 0.0.0.0 --config build/webpack.dev.conf.js",
    "build:rem:components": "webpack --config build/webpack.dist.components.rem.conf.js",
    "build:px:components": "webpack --config build/webpack.dist.components.px.conf.js",
    "build:prod": "webpack --config build/webpack.dist.prod.conf.js",
    "build": "npm run clean && npm run build:rem:components && npm run build:px:components && npm run build:prod && gulp --gulpfile build/build.prod.js"
  },
  "dependencies": {
    "hui-district": "git+https://github.com/HoldSkill/hui-district.git"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.10.1",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^2.3.2",
    "gulp-less": "^3.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^2.0.1",
    "html-loader": "^0.5.1",
    "is": "^3.2.1",
    "json-loader": "^0.5.4",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "moment": "^2.18.1",
    "postcss": "^5.2.14",
    "rimraf": "^2.4.3",
    "string-replace-webpack-plugin": "^0.1.3",
    "style-loader": "^0.13.1",
    "swiper": "^3.4.2",
    "url-loader": "^0.5.8",
    "vue": "^2.5.2",
    "vue-loader": "^10.0.2",
    "vue-resource": "^1.0.3",
    "vue-router": "^2.1.1",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.3",
    "webpack-merge": "^2.4.0",
    "ydui-rem2px": "^1.0.1"
  },
  "license": "MIT"
}
